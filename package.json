{
  "name": "sequalize",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:api": "nodemon --watch backend/**/* -e ts --exec ts-node ./backend/index.ts",
    "dev:fe": "parcel serve ./frontend/index.html",
    "dev:dev": "concurrently \"yarn dev:fe\" \"yarn dev:api\"",
    "dev": "ts-node dev.ts",
    "build:be": "tsc",
    "build:fe": "parcel build ./frontend/index.html --dist-dir ./dist",
    "build": "yarn build:be && yarn build:fe",
    "start": "node ./be-dist/backend/index.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/node": "^18.11.9",
    "@vaadin/router": "^1.7.4",
    "algoliasearch": "^4.14.2",
    "bearer-token": "^2.0.2",
    "cloudinary": "^1.32.0",
    "cors": "^2.8.5",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^2.11.1",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.6",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "buffer": "^5.7.1",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "url": "^0.11.0",
    "util": "^0.12.5"
  }
}
